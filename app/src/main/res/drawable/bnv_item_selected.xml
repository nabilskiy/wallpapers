    android:insetRight="16dp"
    android:insetTop="8dp"
    android:insetBottom="8dp">

    <shape android:shape="rectangle" >
        <corners android:radius="4dp"/>
        <padding android:right="10dp" android:left="10dp" android:top="4dp" android:bottom="4dp"/>
        <solid android:color="#3A3751"/>
        <size android:width="30dp" android:height="32dp"/>
    </shape>
</inset>
